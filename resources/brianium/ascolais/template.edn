 :transform [["build" ""]
             ["src/clj" "src/clj/{{top/file}}" {"core.clj" "{{main/file}}.clj"
                                                "config.clj" "config.clj"
                                                "routes.clj" "routes.clj"} :only]
             ["dev" "dev"]
